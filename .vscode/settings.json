{
     "files.exclude": {
        "**/.git": true,
        "**/.js" : true,
        "**/**.js" : true,
        "**/**.**js" : true,
        "**/**.map" : true,
        "**/**.**.js.map" : { "when": "/**.**$(basename).ts" }
    }
}